class Universidad{
	var ubicacionEnProvincia
	var horariosRecomendado
	
	method ubicacionEnProvincia()= ubicacionEnProvincia
	method ubicacionEnProvincia(prov){ ubicacionEnProvincia = prov }
	
	method horarioRecomendado(hrs){ horariosRecomendado = hrs }
	method horarioRecomendado()= horariosRecomendado
}